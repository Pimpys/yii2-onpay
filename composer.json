{
    "name": "ejen/yii2-onpay",
    "description": "Yii2 onpay extension",
    "keywords": ["yii2", "onpay"],
    "homepage": "https://github.com/ejen/yii2-onpay",
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Dmitry V. Alexeev",
            "email": "mail@alexeevdv.ru",
            "homepage": "https://alexeevdv.ru"
        }
    ],
    "require": {
        "yiisoft/yii2": "^2.0"
    },
    "require-dev": {
        "yiisoft/yii2-coding-standards": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "ejen\\payment\\": "src"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "cp ./pre-commit .git/hooks/pre-commit",
            "chmod +x .git/hooks/pre-commit"
        ]
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
